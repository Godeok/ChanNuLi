<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.co.healthcare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".tutorial.TutorialStartActivity" />
        <activity
            android:name=".tutorial.Tutorial1StepActivity"
            android:windowSoftInputMode="adjustNothing" />
        <activity
            android:name=".tutorial.Tutorial2StepActivity"
            android:windowSoftInputMode="adjustNothing" />
        <activity android:name=".mypage.MypageActivity" />



        <!--자가진단-->
        <activity
            android:name=".self_diagnosis.SelfMainActivity"
            android:noHistory="true" />
        <activity
            android:name=".self_diagnosis.SelfDiagnosisActivity"
            android:noHistory="true" />
        <activity
            android:name=".self_diagnosis.SelfShowResult"
            android:noHistory="true" />
        <activity
            android:name=".self_diagnosis.SelfResultDateActivity"
            android:noHistory="true" />
        <activity
            android:name=".self_diagnosis.SelfResultSymptomActivity"
            android:noHistory="true" />


        <!--게임-->
        <activity android:name=".game.GameMainActivity" />
        <activity android:name=".game.Game1ResultActivity"/>
        <activity
            android:name=".game.Game1Activity"
            android:noHistory="true" />
        <activity android:name=".game.Game2ResultActivity"/>
        <activity android:name=".game.Game2Activity" />
    </application>

</manifest>