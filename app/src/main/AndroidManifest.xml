<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.co.healthcare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".tutorial.TutorialStartActivity" />
        <activity
            android:name=".tutorial.Tutorial1StepActivity"
            android:windowSoftInputMode="adjustNothing" />
        <activity
            android:name=".tutorial.Tutorial2StepActivity"
            android:windowSoftInputMode="adjustNothing" />
        <activity android:name=".mypage.MypageActivity" />


        <activity android:name=".self.SelfMainActivity" />
        <activity android:name=".self.SelfDiagnosisActivity" />
        <activity android:name=".self.SelfShowResult" />
        <activity
            android:name=".self.SelfResultDateActivity"
            android:noHistory="true" />
        <activity
            android:name=".self.SelfResultSymptomActivity"
            android:noHistory="true" />


        <activity
            android:name=".game.GameMainActivity"
            android:noHistory="true" />
        <activity
            android:name=".game.Game1ResultActivity"
            android:noHistory="true" />
        <activity
            android:name=".game.Game1Activity"
            android:noHistory="true" />
        <activity
            android:name=".game.Game2ResultActivity"
            android:noHistory="true" />
        <activity
            android:name=".game.Game2Activity"
            android:noHistory="true" />
    </application>

</manifest>